				{
					"type": "script",
					"language": "javascript",
					"script": [
						"switch(sdk_state){",
						"\tcase SDK_GAMELOFTADS:",
						"",
						"\tvar date = new Date(Date.now());",
						"\tvar curr_day = date.getDay();",
						"\tconsole.log(\"curr_day:\" + curr_day);",
						"",
						"\tif(curr_day != runtime.globalVars.prev_day){",
						"\t\truntime.globalVars.login_count += 1;",
						"\t\truntime.storage.setItem(\"login_count\", runtime.globalVars.login_count);",
						"\t\t",
						"\t\truntime.globalVars.prev_day = curr_day;",
						"\t\truntime.storage.setItem(\"prev_day\", runtime.globalVars.prev_day);",
						"\t\t",
						"\t\tvar d_str = \"\";",
						"\t\t",
						"\t\tif(runtime.globalVars.login_count < 10){",
						"\t\t\td_str = \"D0\" + runtime.globalVars.login_count.toString() + \" - Robots Gone Wild\";",
						"\t\t}",
						"\t\telse{",
						"\t\t\td_str = \"D\" + runtime.globalVars.login_count.toString() + \" - Robots Gone Wild\";",
						"\t\t}",
						"\t\t",
						"\t\twindow.dataLayer.push({",
						"\t\t\t\"event\" : \"ga_event\",",
						"\t\t\t\"ga_category\" : \"Gamepage\", ",
						"\t\t\t\"ga_action\" : \"Return\",",
						"\t\t\t\"ga_label\": d_str,",
						"\t\t\t\"ga_noninteraction\" : true,",
						"\t\t});",
						"\t\t\t\t",
						"\t}",
						"",
						"\tbreak;",
						"}",
						"",
						"",
						""
					]
				}
			],
			"sid": 504972293419952
		},
